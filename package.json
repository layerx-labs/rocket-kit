{
  "version": "3.0.12-0",
  "name": "@taikai/rocket-kit",
  "author": "taikai",
  "description": "TAIKAI Design System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/layerx-labs/rocket-kit"
  },
  "keywords": [
    "react",
    "design-system",
    "taikai",
    "design",
    "component library"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "dotenv cross-var npx chromatic --project-token %CHROMATIC_PROJECT_TOKEN%"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "np": {
    "yarn": true
  },
  "module": "dist/rocket-kit.esm.js",
  "size-limit": [
    {
      "path": "dist/rocket-kit.cjs.production.min.js",
      "limit": "30 KB"
    },
    {
      "path": "dist/rocket-kit.esm.js",
      "limit": "30 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7",
    "@babel/preset-react": "^7",
    "@babel/preset-typescript": "^7",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/react": "^10.1.11",
    "@storybook/react-webpack5": "^10.1.11",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^13.0.7",
    "@types/css-modules": "^1.0.5",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^17",
    "@types/react-beautiful-dnd": "^13",
    "@types/react-dom": "^17",
    "@types/react-paginate": "^7.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "chromatic": "^6.17.4",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.11",
    "eslint-plugin-testing-library": "^6",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.0.1",
    "storybook": "^10.1.11",
    "tsdx": "^0.14.1",
    "tslib": "^2",
    "typescript": "^4.2.3",
    "webpack-cli": "^5.1.4",
    "@storybook/addon-docs": "^10.1.11"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "boring-avatars": "1.6.1",
    "clsx": "^2.1.1",
    "lodash.throttle": "^4.1.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-intersection-observer": "^9",
    "react-paginate": "^7.1.3",
    "react-player": "^2",
    "react-responsive-carousel": "^3.2.18",
    "react-responsive-tabs": "^4.4.1",
    "react-select": "^5",
    "uuid": "^8.3.2"
  },
  "resolutions": {
    "cliui": "^8.0.1",
    "@eslint/eslintrc": "^2.1.4"
  }
}
